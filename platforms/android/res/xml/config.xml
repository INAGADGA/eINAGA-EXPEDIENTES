<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" defaultlocale="es-ES" id="es.aragon.inaeie" version="1.0.0" windows-packageVersion="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <name>eINAGA Expedientes</name>
    <description>Consulta de expedientes tramitados en INAGA en trámite de Información pública y anuncio público . Consulta de Resoluciones dictadas por el Instituto y publicadas en BOA.  Permite la consulta de los datos básicos, los documentos expuestos y la geolocalización de los expedientes. </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Instituto Aragonés de Gestión Ambiental</author>
    <vs:template-name>BlankJS</vs:template-name>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
    <allow-navigation href="about:" />
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget id="es.aragon.inaeie" platformName="windows" version="1.0.0.0" />
    </vs:platformSpecificValues>
    <vs:toolsetVersion>6.3.1</vs:toolsetVersion>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icons/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="res/icons/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="res/icons/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="res/icons/android/icon-96-xhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="10.0" />
    <preference name="WindowsStoreDisplayName" value="eINAGA Expedientes" />
    <preference name="android-maxSdkVersion" value="26" />
    <preference name="ShowTitle" value="False" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="deployment-target" value="9" />
    <preference name="Fullscreen" value="False" />
</widget>
